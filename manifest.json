{
    "manifest_version": 2,

    "name": "IMSLPSkipper",
    "description": "Skip the advertisements on IMSLP",
    "version": "1.0",
    "background": {
        "scripts": ["skip.js"]
    },
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["skip.js"],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_icon": "music.png"
    },
    "permissions": [
        "activeTab"
    ]
}
